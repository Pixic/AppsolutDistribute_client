    <!-- ExpandableListView: Consits of two string arrays one for the group and one for the 
    	 children. The Childrens string array has a divider which is an empty string, has the 
    	 purpose to mark that the children bellow are the children of a new group. 
    	 Note: This solution is because lack of support for 2 dimensional String Arrays. 
    	 DO NOT NAME THE CHILDREN IN THE SAME MENU TO THE SAME THING!!! VERY BAD THINGS WILL HAPPEN -->
    <!-- ExpandableListView menus: -->	 
    <!-- Start menu -->
   <string-array name = "start_groups" >
	    <item>Connect To Server</item>	<!-- group 1 -->
	    <item>Options</item>  			<!-- group 2 -->
	</string-array> 
   
   <string-array name = "start_children" >
   	 <item>Login</item> 				<!-- Here are the children belonging to group 1 -->
   	 <item>Create Account</item>  
   	 <item>Select Server</item>
   	 <item></item> 						<!-- divider: group 1 ends here and group 2 begins -->
   	 <item>About</item>					<!-- Here are the children belonging to group 2 -->
   	 <item>Help</item>
   	 <item>Exit</item>
   </string-array>
   <!-- Help menu, will contain the user's manual -->
    <string-array name = "help_groups" >
