	    <item>2. Second Title</item>  			
	    <item>Options</item>  				    
	</string-array>
	
	<string-array name = "help_children" >
		<item>1.1. First Subtitle</item>
		<item>1.2. Second Subtitle</item>							
   	 	<item></item> 						
   	 	<item>2.1. Subtitle </item>					
